#include <stdio.h>

int main() {
    int num1, num2;
    scanf("%d %d", &num1, &num2);
    printf("Sum=%d, Diff=%d, Product=%d", num1 + num2, num1 - num2, num1 * num2);
    
    if (num2 != 0) {
        printf(", Qt=%.2f\n", (float)num1 / num2);
    } else {
        printf(", Qt=undefined (division by zero)\n");
    }
    return 0;
}

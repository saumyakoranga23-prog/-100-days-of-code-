#include <stdio.h>

char toUpper(char c) {
    if (c >= 'a' && c <= 'z') {
        return c - 32;
    }
    return c;
}

int main() {
    char str[100];
    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);
    
    printf("Uppercase string: ");
    for (int i = 0; str[i] != '\0'; i++) {
        printf("%c", toUpper(str[i]));
    }
    
    return 0;
}

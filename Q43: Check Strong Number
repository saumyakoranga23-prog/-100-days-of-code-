#include <stdio.h>

long long factorial(int n) {
    if (n == 0 || n == 1) return 1;
    long long result = 1;
    for (int i = 2; i <= n; i++) {
        result *= i;
    }
    return result;
}

int isStrong(int n) {
    int original = n;
    long long sum = 0;
    
    while (n > 0) {
        int digit = n % 10;
        sum += factorial(digit);
        n /= 10;
    }
    
    return sum == original;
}

int main() {
    int num;
    printf("Enter a number: ");
    scanf("%d", &num);
    
    if (isStrong(num)) {
        printf("%d is a strong number\n", num);
    } else {
        printf("%d is not a strong number\n", num);
    }
    
    return 0;
}

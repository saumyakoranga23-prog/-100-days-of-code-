#include <stdio.h>

struct Student {
    char name[20];
    int roll;
    int marks;
};

int main() {
    struct Student s[5];
    for (int i = 0; i < 5; i++) {
        printf("Enter Name Roll Marks: ");
        scanf("%s %d %d", s[i].name, &s[i].roll, &s[i].marks);
    }

    printf("\n--- Student List ---\n");
    for (int i = 0; i < 5; i++) {
        printf("Name: %s | Roll: %d | Marks: %d\n", s[i].name, s[i].roll, s[i].marks);
    }
    return 0;
}

#include <stdio.h>

int main() {
    int n;
    printf("Enter a number: ");
    scanf("%d", &n);
    
    printf("Numbers from 1 to %d: ", n);
    for (int i = 1; i <= n; i++) {
        printf("%d ", i);
    }
    printf("\n");
    
    return 0;
}

/*
Sample Test Cases:
Input: 5
Output: 1 2 3 4 5

Input: 3
Output: 1 2 3
*/

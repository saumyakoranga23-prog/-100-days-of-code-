#include <stdio.h>
int binarySearch(int arr[], int n, int x) {
    int l=0, r=n-1;
    while(l<=r) {
        int m = l+(r-l)/2;
        if(arr[m]==x) return m;
        if(arr[m]<x) l=m+1;
        else r=m-1;
    }
    return -1;
}
int main() {
    int arr[] = {2,4,6,8,10}, n=5, x=8;
    printf("%d", binarySearch(arr,n,x));
    return 0;
}

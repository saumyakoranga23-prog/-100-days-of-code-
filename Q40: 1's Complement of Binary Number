#include <stdio.h>
#include <string.h>

void onesComplement(char binary[]) {
    for (int i = 0; binary[i] != '\0'; i++) {
        if (binary[i] == '0') {
            binary[i] = '1';
        } else if (binary[i] == '1') {
            binary[i] = '0';
        }
    }
}

int main() {
    char binary[100];
    printf("Enter a binary number: ");
    scanf("%s", binary);
    
    onesComplement(binary);
    printf("1's complement: %s\n", binary);
    
    return 0;
}
